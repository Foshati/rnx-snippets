{
  "publisher": "Foshati",
  "name": "rnx-snippets",
  "displayName": "RNX Snippets",
  "version": "1.0.2",
  "packageManager": "pnpm@10.22.0",
  "description": "Professional React & Next.js Snippets for JS/JSX/TS/TSX - Boost your productivity",
  "author": "Foshati",
  "license": "MIT",
  "homepage": "https://github.com/Foshati/rnx-snippets#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Foshati/rnx-snippets"
  },
  "bugs": {
    "url": "https://github.com/Foshati/rnx-snippets/issues"
  },
  "keywords": [
    "react",
    "nextjs",
    "snippets",
    "typescript",
    "javascript",
    "jsx",
    "tsx",
    "components",
    "next.js",
    "app router"
  ],
  "categories": [
    "Snippets"
  ],
  "icon": "public/icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "typescriptreact",
        "path": "./src/snippets/ts-snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./src/snippets/js-snippets.json"
      },
      {
        "language": "typescript",
        "path": "./src/snippets/ts-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./src/snippets/js-snippets.json"
      }
    ]
  },
  "scripts": {
    "test": "vitest",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@vscode/vsce": "^3.7.0",
    "typescript": "^5.8.3",
    "vitest": "^4.0.16"
  }
}