    "description": "React useReducer Hook"
  },
  "React Custom Hook": {
    "prefix": "jsh",
    "body": [
      "import { useState, useEffect } from 'react'",
      "",
      "export function use${1:CustomHook}(${2:params}) {",
      "  const [${3:state}, set${3/(.*)/${1:/capitalize}/}] = useState(${4:initialValue})",
      "",
      "  useEffect(() => {",
      "    ${5:// effect logic}",
      "  }, [${6:dependencies}])",
      "",
      "  return { ${3:state} }",
      "}"
    ],
    "description": "React Custom Hook Template"
  },
  "React Context Provider": {
    "prefix": "jsctx",
    "body": [
      "import { createContext, useContext, useState } from 'react'",
      "",
      "const ${1:MyContext} = createContext(undefined)",
      "",
      "export function ${1:MyContext}Provider({ children }) {",
      "  const [${2:value}, set${2/(.*)/${1:/capitalize}/}] = useState('${3:}')",
      "",
      "  return (",
      "    <${1:MyContext}.Provider value={{ ${2:value}, set${2/(.*)/${1:/capitalize}/} }}>",
      "      {children}",
      "    </${1:MyContext}.Provider>",
      "  )",
      "}",
      "",
      "export function use${1:MyContext}() {",
      "  const context = useContext(${1:MyContext})",
      "  if (!context) {",
      "    throw new Error('use${1:MyContext} must be used within ${1:MyContext}Provider')",
      "  }",
      "  return context",
      "}"
    ],
    "description": "React Context with Provider and Hook"
  },

