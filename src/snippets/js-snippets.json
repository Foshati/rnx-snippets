  "React useState Hook": {
    "prefix": "us",
    "body": [
      "const [${1:state}, set${1/(.*)/${1:/capitalize}/}] = useState(${2:initialValue})"
    ],
    "description": "React useState Hook"
  },
  "React useEffect Hook": {
    "prefix": "ue",
    "body": [
      "useEffect(() => {",
      "  ${1:// effect}",
      "  ${2:return () => {",
      "    // cleanup",
      "  \\}}",
      "}, [${3:dependencies}])"
    ],
    "description": "React useEffect Hook"
  },
  "React useEffect Mount": {
    "prefix": "uem",
    "body": [
      "useEffect(() => {",
      "  ${1:// runs once on mount}",
      "}, [])"
    ],
    "description": "React useEffect on Mount (empty deps)"
  },
  "React useContext Hook": {
    "prefix": "uc",
    "body": [
