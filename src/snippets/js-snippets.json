  "Next.js Loading": {
    "prefix": "jsload",
    "body": [
      "export default function Loading() {",
      "  return (",
      "    <div className=\"${1:flex items-center justify-center min-h-screen}\">",
      "      <div className=\"${2:animate-spin rounded-full h-12 w-12 border-t-2 border-b-2 border-primary}\"></div>",
      "    </div>",
      "  )",
      "}"
    ],
    "description": "Next.js Loading Component"
  },
  "Next.js Error": {
    "prefix": "jserr",
    "body": [
      "'use client'",
      "",
      "import { useEffect } from 'react'",
      "",
      "export default function Error({ error, reset }) {",
      "  useEffect(() => {",
      "    console.error(error)",
      "  }, [error])",
      "",
      "  return (",
      "    <div className=\"${1:flex flex-col items-center justify-center min-h-screen gap-4}\">",
      "      <h2 className=\"${2:text-xl font-semibold}\">Something went wrong!</h2>",
      "      <button",
      "        onClick={reset}",
      "        className=\"${3:px-4 py-2 bg-primary text-white rounded-md}\"",
      "      >",
      "        Try again",
      "      </button>",
      "    </div>",
      "  )",
      "}"
    ],
    "description": "Next.js Error Component"
  },
  "Next.js Not Found": {
    "prefix": "js404",
    "body": [
      "import Link from 'next/link'",
      "",
      "export default function NotFound() {",
      "  return (",
      "    <div className=\"${1:flex flex-col items-center justify-center min-h-screen gap-4}\">",
      "      <h2 className=\"${2:text-2xl font-bold}\">404 - Not Found</h2>",
      "      <p className=\"${3:text-muted-foreground}\">Could not find requested resource</p>",
      "      <Link href=\"/\" className=\"${4:text-primary hover:underline}\">",
      "        Return Home",
      "      </Link>",
      "    </div>",
      "  )",
      "}"
    ],
    "description": "Next.js Not Found Component"
  },
  "Next.js API Route": {
    "prefix": "jsapi",
    "body": [
      "import { NextResponse } from 'next/server'",
      "",
      "export async function ${1|GET,POST,PUT,DELETE,PATCH|}(request) {",
      "  try {",
      "    ${2:// Your logic here}",
      "",
      "    return NextResponse.json({ ${3:message: 'Success'} })",
      "  } catch (error) {",
      "    return NextResponse.json(",
      "      { error: 'Internal Server Error' },",
      "      { status: 500 }",
      "    )",
