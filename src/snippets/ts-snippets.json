            "}"
        ],
        "description": "Next.js Loading Component"
    },
    "Next.js Error": {
        "prefix": "tserr",
        "body": [
            "'use client'",
            "",
            "import { useEffect } from 'react'",
            "",
            "type Props = {",
            "  error: Error & { digest?: string }",
            "  reset: () => void",
            "}",
            "",
            "export default function Error({ error, reset }: Props) {",
            "  useEffect(() => {",
            "    console.error(error)",
            "  }, [error])",
            "",
            "  return (",
            "    <div className=\"${1:flex flex-col items-center justify-center min-h-screen gap-4}\">",
            "      <h2 className=\"${2:text-xl font-semibold}\">Something went wrong!</h2>",
            "      <button",
            "        onClick={reset}",
            "        className=\"${3:px-4 py-2 bg-primary text-white rounded-md}\"",
            "      >",
            "        Try again",
            "      </button>",
            "    </div>",
            "  )",
            "}"
        ],
        "description": "Next.js Error Component"
    },
