        "body": [
            "export default function Loading() {",
            "  return (",
            "    <div className=\"${1:flex items-center justify-center min-h-screen}\">",
            "      <div className=\"${2:animate-spin rounded-full h-12 w-12 border-t-2 border-b-2 border-primary}\"></div>",
