    "React Custom Hook": {
        "prefix": "tsh",
        "body": [
            "import { useState, useEffect } from 'react'",
            "",
            "export function use${1:CustomHook}(${2:params}: ${3:ParamsType}) {",
            "  const [${4:state}, set${4/(.*)/${1:/capitalize}/}] = useState<${5:StateType}>(${6:initialValue})",
            "",
            "  useEffect(() => {",
            "    ${7:// effect logic}",
            "  }, [${8:dependencies}])",
            "",
            "  return { ${4:state} }",
            "}"
        ],
        "description": "React Custom Hook Template"
    },
    "React Context Provider": {
        "prefix": "tsctx",
        "body": [
            "import { createContext, useContext, useState, ReactNode } from 'react'",
            "",
            "type ${1:MyContext}Type = {",
            "  ${2:value}: ${3:string}",
            "  ${4:setValue}: (value: ${3:string}) => void",
            "}",
            "",
            "const ${1:MyContext} = createContext<${1:MyContext}Type | undefined>(undefined)",
            "",
            "export function ${1:MyContext}Provider({ children }: { children: ReactNode }) {",
            "  const [${2:value}, set${2/(.*)/${1:/capitalize}/}] = useState<${3:string}>('${5:}')",
            "",
            "  return (",
            "    <${1:MyContext}.Provider value={{ ${2:value}, ${4:setValue}: set${2/(.*)/${1:/capitalize}/} }}>",
            "      {children}",
            "    </${1:MyContext}.Provider>",
            "  )",
            "}",
            "",
            "export function use${1:MyContext}() {",
            "  const context = useContext(${1:MyContext})",
            "  if (!context) {",
            "    throw new Error('use${1:MyContext} must be used within ${1:MyContext}Provider')",
            "  }",
            "  return context",
            "}"
        ],
        "description": "React Context with Provider and Hook"
    },

